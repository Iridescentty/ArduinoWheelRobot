int r2=25;
int rc=8;

//左轮引脚
int l2=27;
int l1=29;
int lc=10;

//前轮引脚
int f1=37;
int f2=35;
int fc=11;

//速度
double speed=10;
String words;

String num;

void setup()
{
	Serial.begin(9600);
	pinMode(l1,OUTPUT);
	pinMode(l2,OUTPUT);
	pinMode(lc,OUTPUT);
	pinMode(r1,OUTPUT);
	pinMode(r2,OUTPUT);
	pinMode(rc,OUTPUT);
	pinMode(f1,OUTPUT);
	pinMode(f2,OUTPUT);
	pinMode(fc,OUTPUT);
}

void loop()
{
	if(Serial.available()>0)
	{
		while(Serial.available()>0)
		{
			words+=(char)(Serial.read());
			delay(2);
		}
		num=words;
		words="";

		Serial.println();
		Serial.print("num==");
		Serial.print(num);
	}
	if(num=="a"){Go_ahead();}
	if(num=="b"){Go_back();}
	if(num=="tr"){Turn_right();}
	if(num=="gr"){Go_right();}
	if(num=="br"){Back_Turn_right();}
	if(num=="tl"){Turn_Ligft();}
	if(num=="gl"){Go_ligft();}
	if(num=="bl"){Back_Turn_ligft();}
	if(num=="r"){Right();}
	if(num=="l"){Light();}
	if(num=="s"){Stop();}
}

